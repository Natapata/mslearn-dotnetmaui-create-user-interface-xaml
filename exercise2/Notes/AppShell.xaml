<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Notes.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Notes">

    <ShellContent
        Title="Alignment"
        ContentTemplate="{DataTemplate local:AlignmentPage}"
        Route="AlignmentPage" />
    <ShellContent
        Title="LayoutExample"
        ContentTemplate="{DataTemplate local:LayoutExamplePage}"
        Route="LayoutExamplePage" />
    <ShellContent
        Title="TipCalculator"
        ContentTemplate="{DataTemplate local:TipCalulatorPage}"
        Route="TipCalculatorPage" />
    <ShellContent
        Title="Notes"
        ContentTemplate="{DataTemplate local:NotesPage}"
        Route="NotesPage" />



</Shell>
